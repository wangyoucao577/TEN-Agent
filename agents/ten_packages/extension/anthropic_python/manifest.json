{
  "type": "extension",
  "name": "anthropic_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.2"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md"
    ]
  },
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "model": {
        "type": "string"
      },
      "max_tokens": {
        "type": "int64"
      },
      "prompt": {
        "type": "string"
      },
      "greeting": {
        "type": "string"
      }
    }
  }
}